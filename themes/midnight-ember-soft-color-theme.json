{
    "$schema": "vscode://schemas/color-theme",
    "name": "Midnight Ember Soft",
    "type": "dark",
    "semanticHighlighting": true,
    "semanticTokenColors": {
        "variable.readonly": "#D4D4D8",
        "variable.defaultLibrary": "#89B4FA",
        "property.readonly": "#D4D4D8",
        "namespace": "#FFB86C",
        "type": "#89DCEB",
        "type.defaultLibrary": "#89DCEB",
        "class": "#89DCEB",
        "interface": "#89DCEB",
        "enum": "#89DCEB",
        "enumMember": "#FFB86C",
        "function": "#94E2D5",
        "method": "#94E2D5",
        "macro": "#F38BA8",
        "parameter": "#FFB86C",
        "property": "#D4D4D8",
        "variable": "#D4D4D8"
    },
    "colors": {
        // ─── Base Colors ────────────────────────────────
        "foreground": "#D4D4D8",
        "disabledForeground": "#52525B",
        "focusBorder": "#F5A97F00",
        "selection.background": "#F5A97F40",
        "descriptionForeground": "#A1A1AA",
        "errorForeground": "#EF4444",
        "icon.foreground": "#A1A1AA",
        "widget.shadow": "#00000050",
        "widget.border": "#27272A",
        // ─── Window ─────────────────────────────────────
        "window.activeBorder": "#00000000",
        "window.inactiveBorder": "#00000000",
        // ─── Text Colors ────────────────────────────────
        "textLink.foreground": "#F5A97F",
        "textLink.activeForeground": "#FFB86C",
        "textBlockQuote.background": "#18181B",
        "textBlockQuote.border": "#F5A97F",
        "textPreformat.foreground": "#D4D4D8",
        "textSeparator.foreground": "#27272A",
        // ─── Button ─────────────────────────────────────
        "button.background": "#F5A97F",
        "button.foreground": "#18181B",
        "button.hoverBackground": "#FFB86C",
        "button.secondaryBackground": "#27272A",
        "button.secondaryForeground": "#D4D4D8",
        "button.secondaryHoverBackground": "#3F3F46",
        "button.border": "#00000000",
        // ─── Dropdown ───────────────────────────────────
        "dropdown.background": "#1E1E24",
        "dropdown.border": "#3F3F46",
        "dropdown.foreground": "#D4D4D8",
        "dropdown.listBackground": "#1E1E24",
        // ─── Input ──────────────────────────────────────
        "input.background": "#18181B",
        "input.border": "#3F3F46",
        "input.foreground": "#D4D4D8",
        "input.placeholderForeground": "#52525B",
        "inputOption.activeBackground": "#F5A97F33",
        "inputOption.activeBorder": "#F5A97F",
        "inputOption.activeForeground": "#D4D4D8",
        "inputValidation.errorBackground": "#450A0A",
        "inputValidation.errorBorder": "#F38BA8",
        "inputValidation.infoBackground": "#18181B",
        "inputValidation.infoBorder": "#89DCEB",
        "inputValidation.warningBackground": "#422006",
        "inputValidation.warningBorder": "#F5A97F",
        // ─── Scrollbar ──────────────────────────────────
        "scrollbar.shadow": "#00000050",
        "scrollbarSlider.activeBackground": "#F5A97F66",
        "scrollbarSlider.background": "#D4D4D820",
        "scrollbarSlider.hoverBackground": "#D4D4D840",
        // ─── Badge ──────────────────────────────────────
        "badge.background": "#F5A97F",
        "badge.foreground": "#18181B",
        // ─── Progress Bar ───────────────────────────────
        "progressBar.background": "#F5A97F",
        // ─── Lists ──────────────────────────────────────
        "list.activeSelectionBackground": "#F5A97F20",
        "list.activeSelectionForeground": "#D4D4D8",
        "list.dropBackground": "#F5A97F15",
        "list.focusBackground": "#F5A97F20",
        "list.focusForeground": "#D4D4D8",
        "list.focusOutline": "#F5A97F80",
        "list.highlightForeground": "#F5A97F",
        "list.hoverBackground": "#1E1E2480",
        "list.hoverForeground": "#D4D4D8",
        "list.inactiveSelectionBackground": "#F5A97F15",
        "list.inactiveSelectionForeground": "#D4D4D8",
        "list.warningForeground": "#FFB86C",
        "list.errorForeground": "#EF4444",
        // ─── Activity Bar ───────────────────────────────
        "activityBar.background": "#18181B",
        "activityBar.foreground": "#D4D4D8",
        "activityBar.inactiveForeground": "#52525B",
        "activityBar.border": "#27272A",
        "activityBar.activeBorder": "#F5A97F",
        "activityBar.activeFocusBorder": "#F5A97F",
        "activityBarBadge.background": "#F5A97F",
        "activityBarBadge.foreground": "#18181B",
        // ─── Side Bar ───────────────────────────────────
        "sideBar.background": "#18181B",
        "sideBar.foreground": "#A1A1AA",
        "sideBar.border": "#27272A",
        "sideBarTitle.foreground": "#D4D4D8",
        "sideBarSectionHeader.background": "#18181B",
        "sideBarSectionHeader.foreground": "#D4D4D8",
        "sideBarSectionHeader.border": "#27272A",
        // ─── Tab (Editor Group) ─────────────────────────
        "editorGroupHeader.tabsBackground": "#18181B",
        "editorGroupHeader.tabsBorder": "#27272A",
        "editorGroup.border": "#27272A",
        "tab.activeBackground": "#1E1E24",
        "tab.activeForeground": "#D4D4D8",
        "tab.activeBorderTop": "#F5A97F",
        "tab.activeBorder": "#1E1E24",
        "tab.border": "#27272A",
        "tab.hoverBackground": "#1E1E2480",
        "tab.inactiveBackground": "#18181B",
        "tab.inactiveForeground": "#52525B",
        // ─── Editor ─────────────────────────────────────
        "editor.background": "#1A1A20",
        "editor.foreground": "#D4D4D8",
        "editor.foldBackground": "#F5A97F10",
        "editor.lineHighlightBackground": "#27272A60",
        "editor.lineHighlightBorder": "#27272A00",
        "editor.selectionBackground": "#F5A97F30",
        "editor.selectionHighlightBackground": "#F5A97F18",
        "editor.inactiveSelectionBackground": "#F5A97F15",
        "editor.wordHighlightBackground": "#F5A97F18",
        "editor.wordHighlightStrongBackground": "#F5A97F25",
        "editor.findMatchBackground": "#F5A97F40",
        "editor.findMatchBorder": "#F5A97F",
        "editor.findMatchHighlightBackground": "#F5A97F25",
        "editor.findRangeHighlightBackground": "#F5A97F10",
        "editor.hoverHighlightBackground": "#F5A97F15",
        "editorLink.activeForeground": "#F5A97F",
        // ─── Editor: Cursor ─────────────────────────────
        "editorCursor.background": "#1A1A20",
        "editorCursor.foreground": "#F5A97F",
        // ─── Editor: Whitespace ─────────────────────────
        "editorWhitespace.foreground": "#27272A",
        // ─── Editor: Indent Guide ───────────────────────
        "editorIndentGuide.activeBackground1": "#3F3F46",
        "editorIndentGuide.background1": "#27272A",
        // ─── Editor: Line Numbers ───────────────────────
        "editorLineNumber.foreground": "#3F3F46",
        "editorLineNumber.activeForeground": "#F5A97F",
        // ─── Editor: Bracket Match ──────────────────────
        "editorBracketMatch.background": "#F5A97F20",
        "editorBracketMatch.border": "#F5A97F80",
        // ─── Editor: Bracket Pair ───────────────────────
        "editorBracketHighlight.foreground1": "#F5A97F",
        "editorBracketHighlight.foreground2": "#89DCEB",
        "editorBracketHighlight.foreground3": "#CBA6F7",
        "editorBracketHighlight.foreground4": "#94E2D5",
        "editorBracketHighlight.foreground5": "#FFB86C",
        "editorBracketHighlight.foreground6": "#F38BA8",
        // ─── Editor: Gutter ─────────────────────────────
        "editorGutter.addedBackground": "#A6E3A160",
        "editorGutter.deletedBackground": "#F38BA860",
        "editorGutter.modifiedBackground": "#F5A97F60",
        // ─── Editor Widget ──────────────────────────────
        "editorWidget.background": "#1E1E24",
        "editorWidget.foreground": "#D4D4D8",
        "editorWidget.border": "#3F3F46",
        "editorSuggestWidget.background": "#1E1E24",
        "editorSuggestWidget.border": "#3F3F46",
        "editorSuggestWidget.foreground": "#D4D4D8",
        "editorSuggestWidget.focusHighlightForeground": "#F5A97F",
        "editorSuggestWidget.highlightForeground": "#F5A97F",
        "editorSuggestWidget.selectedBackground": "#F5A97F20",
        "editorHoverWidget.background": "#1E1E24",
        "editorHoverWidget.border": "#3F3F46",
        // ─── Panel ──────────────────────────────────────
        "panel.background": "#18181B",
        "panel.border": "#27272A",
        "panelTitle.activeBorder": "#F5A97F",
        "panelTitle.activeForeground": "#D4D4D8",
        "panelTitle.inactiveForeground": "#52525B",
        // ─── Status Bar ─────────────────────────────────
        "statusBar.background": "#18181B",
        "statusBar.foreground": "#A1A1AA",
        "statusBar.border": "#27272A",
        "statusBar.debuggingBackground": "#F5A97F",
        "statusBar.debuggingForeground": "#18181B",
        "statusBar.noFolderBackground": "#18181B",
        "statusBarItem.remoteBackground": "#F5A97F",
        "statusBarItem.remoteForeground": "#18181B",
        // ─── Title Bar ──────────────────────────────────
        "titleBar.activeBackground": "#18181B",
        "titleBar.activeForeground": "#D4D4D8",
        "titleBar.inactiveBackground": "#18181B",
        "titleBar.inactiveForeground": "#52525B",
        "titleBar.border": "#27272A",
        // ─── Terminal ───────────────────────────────────
        "terminal.background": "#18181B",
        "terminal.foreground": "#D4D4D8",
        "terminal.border": "#27272A",
        "terminal.selectionBackground": "#F5A97F30",
        "terminalCursor.foreground": "#F5A97F",
        "terminal.ansiBlack": "#18181B",
        "terminal.ansiBrightBlack": "#52525B",
        "terminal.ansiRed": "#F38BA8",
        "terminal.ansiBrightRed": "#F5A0B8",
        "terminal.ansiGreen": "#A6E3A1",
        "terminal.ansiBrightGreen": "#B8F0B0",
        "terminal.ansiYellow": "#F5A97F",
        "terminal.ansiBrightYellow": "#FFB86C",
        "terminal.ansiBlue": "#89DCEB",
        "terminal.ansiBrightBlue": "#A0E7F3",
        "terminal.ansiMagenta": "#CBA6F7",
        "terminal.ansiBrightMagenta": "#DDB8FF",
        "terminal.ansiCyan": "#94E2D5",
        "terminal.ansiBrightCyan": "#A8F0E0",
        "terminal.ansiWhite": "#D4D4D8",
        "terminal.ansiBrightWhite": "#FAFAFA",
        // ─── Git Decoration ─────────────────────────────
        "gitDecoration.addedResourceForeground": "#A6E3A1",
        "gitDecoration.conflictingResourceForeground": "#FFB86C",
        "gitDecoration.deletedResourceForeground": "#C75050",
        "gitDecoration.ignoredResourceForeground": "#52525B",
        "gitDecoration.modifiedResourceForeground": "#E5C07B",
        "gitDecoration.renamedResourceForeground": "#89DCEB",
        "gitDecoration.untrackedResourceForeground": "#A6E3A1",
        // ─── Breadcrumb ─────────────────────────────────
        "breadcrumb.background": "#1E1E24",
        "breadcrumb.foreground": "#A1A1AA",
        "breadcrumb.focusForeground": "#D4D4D8",
        "breadcrumb.activeSelectionForeground": "#D4D4D8",
        // ─── Diff Editor ────────────────────────────────
        "diffEditor.insertedTextBackground": "#A6E3A115",
        "diffEditor.insertedLineBackground": "#A6E3A115",
        "diffEditor.removedTextBackground": "#F38BA815",
        "diffEditor.removedLineBackground": "#F38BA815",
        // ─── Minimap ────────────────────────────────────
        "minimap.background": "#18181B",
        "minimap.findMatchHighlight": "#F5A97F66",
        "minimap.selectionHighlight": "#F5A97F44",
        // ─── Sticky Scroll ──────────────────────────────
        "editorStickyScroll.background": "#18181B",
        "editorStickyScrollHover.background": "#1E1E24",
        // ─── Quick Input ────────────────────────────────
        "quickInput.background": "#1E1E24",
        "quickInput.foreground": "#D4D4D8",
        "quickInputList.focusBackground": "#F5A97F20",
        // ─── Notifications ──────────────────────────────
        "notifications.background": "#1E1E24",
        "notifications.border": "#3F3F46",
        "notifications.foreground": "#D4D4D8",
        "notificationLink.foreground": "#F5A97F",
        // ─── Welcome Page ───────────────────────────────
        "welcomePage.tileBackground": "#1E1E24",
        "welcomePage.tileBorder": "#27272A",
        "welcomePage.tileHoverBackground": "#27272A",
        // ─── Settings ───────────────────────────────────
        "settings.modifiedItemIndicator": "#F5A97F",
        "settings.headerForeground": "#D4D4D8",
        // ─── Menu ───────────────────────────────────────
        "menu.background": "#1E1E24",
        "menu.border": "#3F3F46",
        "menu.foreground": "#D4D4D8",
        "menu.selectionBackground": "#F5A97F20",
        "menu.selectionForeground": "#D4D4D8",
        "menu.separatorBackground": "#27272A",
        // ─── Debug ──────────────────────────────────────
        "debugToolBar.background": "#1E1E24",
        "debugIcon.breakpointForeground": "#F38BA8",
        "debugIcon.continueForeground": "#A6E3A1",
        "debugIcon.startForeground": "#A6E3A1",
        "debugIcon.stopForeground": "#F38BA8",
        "debugConsole.errorForeground": "#F38BA8",
        "debugConsole.infoForeground": "#89DCEB",
        "debugConsole.warningForeground": "#FFB86C",
        // ─── Peek View ──────────────────────────────────
        "peekView.border": "#F5A97F",
        "peekViewEditor.background": "#18181B",
        "peekViewResult.background": "#1E1E24",
        "peekViewTitle.background": "#1E1E24",
        // ─── Testing ────────────────────────────────────
        "testing.iconErrored": "#F38BA8",
        "testing.iconFailed": "#F38BA8",
        "testing.iconPassed": "#A6E3A1",
        "testing.iconQueued": "#F5A97F",
        "testing.runAction": "#A6E3A1",
        // ─── Charts ─────────────────────────────────────
        "charts.foreground": "#D4D4D8",
        "charts.blue": "#89DCEB",
        "charts.green": "#A6E3A1",
        "charts.orange": "#F5A97F",
        "charts.purple": "#CBA6F7",
        "charts.red": "#F38BA8",
        "charts.yellow": "#FFB86C"
    },
    "tokenColors": [
        // ─── Comments ───────────────────────────────────
        {
            "name": "Comments",
            "scope": [
                "comment",
                "punctuation.definition.comment"
            ],
            "settings": {
                "foreground": "#52525B",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Doc Comments",
            "scope": [
                "comment.block.documentation",
                "comment.block.javadoc",
                "string.quoted.docstring"
            ],
            "settings": {
                "foreground": "#71717A",
                "fontStyle": "italic"
            }
        },
        // ─── Keywords ───────────────────────────────────
        {
            "name": "Keywords",
            "scope": [
                "keyword",
                "keyword.control",
                "keyword.operator.new",
                "keyword.operator.expression",
                "keyword.operator.logical",
                "keyword.operator.delete",
                "keyword.operator.typeof",
                "keyword.operator.void",
                "keyword.operator.instanceof",
                "keyword.operator.in",
                "keyword.operator.of"
            ],
            "settings": {
                "foreground": "#F5A97F"
            }
        },
        {
            "name": "Control Flow Keywords",
            "scope": [
                "keyword.control.conditional",
                "keyword.control.loop",
                "keyword.control.flow",
                "keyword.control.trycatch",
                "keyword.control.switch",
                "keyword.control.import",
                "keyword.control.export",
                "keyword.control.from",
                "keyword.control.as",
                "keyword.control.default"
            ],
            "settings": {
                "foreground": "#F5A97F",
                "fontStyle": "italic"
            }
        },
        // ─── Storage ────────────────────────────────────
        {
            "name": "Storage",
            "scope": [
                "storage",
                "storage.type",
                "storage.modifier"
            ],
            "settings": {
                "foreground": "#F5A97F"
            }
        },
        {
            "name": "Storage Type",
            "scope": [
                "storage.type.function",
                "storage.type.class",
                "storage.type.interface",
                "storage.type.enum",
                "storage.type.type"
            ],
            "settings": {
                "foreground": "#F5A97F",
                "fontStyle": "italic"
            }
        },
        // ─── Functions ──────────────────────────────────
        {
            "name": "Function Declarations",
            "scope": [
                "entity.name.function",
                "meta.function-call entity.name.function",
                "support.function",
                "meta.function-call",
                "meta.method-call"
            ],
            "settings": {
                "foreground": "#94E2D5"
            }
        },
        {
            "name": "Function Parameters",
            "scope": [
                "variable.parameter",
                "meta.parameter"
            ],
            "settings": {
                "foreground": "#FFB86C",
                "fontStyle": "italic"
            }
        },
        // ─── Variables ──────────────────────────────────
        {
            "name": "Variables",
            "scope": [
                "variable",
                "variable.other",
                "variable.other.readwrite"
            ],
            "settings": {
                "foreground": "#D4D4D8"
            }
        },
        {
            "name": "Language Variables (this, self, super)",
            "scope": [
                "variable.language",
                "variable.language.this",
                "variable.language.self",
                "variable.language.super"
            ],
            "settings": {
                "foreground": "#FFB86C",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Constants",
            "scope": [
                "variable.other.constant",
                "constant.other",
                "support.constant"
            ],
            "settings": {
                "foreground": "#89B4FA"
            }
        },
        // ─── Strings ────────────────────────────────────
        {
            "name": "Strings",
            "scope": [
                "string",
                "string.quoted",
                "string.template",
                "string.quoted.single",
                "string.quoted.double",
                "string.quoted.triple"
            ],
            "settings": {
                "foreground": "#F38BA8"
            }
        },
        {
            "name": "String Interpolation",
            "scope": [
                "string.template meta.template.expression",
                "punctuation.definition.template-expression",
                "punctuation.section.embedded"
            ],
            "settings": {
                "foreground": "#F5A97F"
            }
        },
        {
            "name": "String Escape Characters",
            "scope": [
                "constant.character.escape"
            ],
            "settings": {
                "foreground": "#89DCEB"
            }
        },
        // ─── Numbers & Booleans ─────────────────────────
        {
            "name": "Numbers",
            "scope": [
                "constant.numeric",
                "constant.numeric.integer",
                "constant.numeric.float",
                "constant.numeric.hex"
            ],
            "settings": {
                "foreground": "#FFB86C"
            }
        },
        {
            "name": "Booleans & Null",
            "scope": [
                "constant.language",
                "constant.language.boolean",
                "constant.language.null",
                "constant.language.undefined"
            ],
            "settings": {
                "foreground": "#FFB86C"
            }
        },
        // ─── Classes & Types ────────────────────────────
        {
            "name": "Class Names",
            "scope": [
                "entity.name.type",
                "entity.name.type.class",
                "entity.name.type.interface",
                "entity.name.type.enum",
                "entity.name.type.alias",
                "support.class",
                "support.type"
            ],
            "settings": {
                "foreground": "#89DCEB"
            }
        },
        {
            "name": "Type Parameters (Generics)",
            "scope": [
                "entity.name.type.parameter"
            ],
            "settings": {
                "foreground": "#CBA6F7"
            }
        },
        {
            "name": "Inherited Class",
            "scope": [
                "entity.other.inherited-class"
            ],
            "settings": {
                "foreground": "#89DCEB",
                "fontStyle": "italic"
            }
        },
        // ─── Operators ──────────────────────────────────
        {
            "name": "Operators",
            "scope": [
                "keyword.operator",
                "keyword.operator.assignment",
                "keyword.operator.arithmetic",
                "keyword.operator.comparison",
                "keyword.operator.relational"
            ],
            "settings": {
                "foreground": "#89DCEB"
            }
        },
        {
            "name": "Arrow Function",
            "scope": [
                "storage.type.function.arrow",
                "keyword.operator.arrow"
            ],
            "settings": {
                "foreground": "#F5A97F"
            }
        },
        // ─── Punctuation ────────────────────────────────
        {
            "name": "Punctuation",
            "scope": [
                "punctuation",
                "punctuation.definition",
                "punctuation.separator",
                "punctuation.terminator",
                "meta.brace",
                "punctuation.accessor"
            ],
            "settings": {
                "foreground": "#A1A1AA"
            }
        },
        {
            "name": "Punctuation - String",
            "scope": [
                "punctuation.definition.string",
                "punctuation.definition.string.begin",
                "punctuation.definition.string.end"
            ],
            "settings": {
                "foreground": "#F38BA8"
            }
        },
        // ─── Decorators / Annotations ───────────────────
        {
            "name": "Decorators",
            "scope": [
                "meta.decorator",
                "meta.decorator entity.name.function",
                "punctuation.decorator"
            ],
            "settings": {
                "foreground": "#FFB86C",
                "fontStyle": "italic"
            }
        },
        // ─── Tags (HTML/JSX/XML) ────────────────────────
        {
            "name": "HTML Tags",
            "scope": [
                "entity.name.tag",
                "punctuation.definition.tag"
            ],
            "settings": {
                "foreground": "#F38BA8"
            }
        },
        {
            "name": "HTML Tag Attributes",
            "scope": [
                "entity.other.attribute-name"
            ],
            "settings": {
                "foreground": "#F5A97F",
                "fontStyle": "italic"
            }
        },
        {
            "name": "JSX Component Tags",
            "scope": [
                "support.class.component"
            ],
            "settings": {
                "foreground": "#89DCEB"
            }
        },
        // ─── CSS ────────────────────────────────────────
        {
            "name": "CSS Properties",
            "scope": [
                "support.type.property-name.css"
            ],
            "settings": {
                "foreground": "#89DCEB"
            }
        },
        {
            "name": "CSS Values",
            "scope": [
                "support.constant.property-value.css"
            ],
            "settings": {
                "foreground": "#FFB86C"
            }
        },
        {
            "name": "CSS Selectors",
            "scope": [
                "entity.other.attribute-name.class.css",
                "entity.other.attribute-name.id.css",
                "entity.name.tag.css"
            ],
            "settings": {
                "foreground": "#94E2D5"
            }
        },
        {
            "name": "CSS Pseudo",
            "scope": [
                "entity.other.attribute-name.pseudo-class.css",
                "entity.other.attribute-name.pseudo-element.css"
            ],
            "settings": {
                "foreground": "#CBA6F7",
                "fontStyle": "italic"
            }
        },
        // ─── JSON ───────────────────────────────────────
        {
            "name": "JSON Keys",
            "scope": [
                "support.type.property-name.json"
            ],
            "settings": {
                "foreground": "#89DCEB"
            }
        },
        {
            "name": "JSON String Values",
            "scope": [
                "string.quoted.double.json"
